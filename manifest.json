{
  "name": "Redirect",
  "version": "0.0.1",
  "description": "Fix Display of Webpages Without Stylesheets",
  "background": { 
     "background_page":"bg.html"
     },
   "manifest_version": 2,
   "browser_action": {
    "default_icon": "icon.png"
   },
  "permissions": ["tabs",
    "http://*/*",
    "https://*/*"]
}